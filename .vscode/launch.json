  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "protocol": "inspector",
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["dev:debug"],
      "name": "Launch",
      "protocol": "inspector",
      "restart": true,
      "port": 9229
    }
  ]
}
